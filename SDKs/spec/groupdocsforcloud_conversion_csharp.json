  "definitions": {
    "CellsConversionSettings": {
      "type": "null",
      "additionalProperties": false,
      "allOf": [
        {
          "$ref": "#/definitions/BaseConversionRequest"
        },
        {
          "type": "object",
          "properties": {
            "Options": {
              "description": "The settings",
              "$ref": "#/definitions/CellsSaveOptionsDto"
            }
          }
        }
      ],
      "description": "CellConversionRequest "
    },
    "CellsSaveOptionsDto": {
      "type": "null",
      "additionalProperties": false,
      "required": [
        "ConvertFileType"
      ],
      "allOf": [
        {
          "$ref": "#/definitions/SaveOptionsBaseDto"
        },
        {
          "type": "object",
          "properties": {
            "ConvertFileType": {
              "enum": [
                "Xls",
                "Xlsx",
                "Xls2003",
                "Xlsm",
                "Xlsb",
                "Ods",
                "Csv"
              ],
              "description": "Get or set the desired cells format "
            },
            "Password": {
              "type": "string",
              "description": "Set this property if you want to protect the converted document with a password"
            }
          }
        }
      ],
      "description": "Cells save options"
    },
    "SaveOptionsBaseDto": {
      "type": "object",
      "discriminator": "Type",
      "additionalProperties": false,
      "required": [
        "PageNumber",
        "NumPagesToConvert",
        "ShowGridLines",
        "ShowHiddenSheets",
        "HideWordTrackedChanges",
        "HidePdfAnnotations",
        "HideComments"
      ],
      "properties": {
        "PageNumber": {
          "type": "integer",
          "description": "Start conversion from PageNumber page",
          "format": "int32"
        },
        "NumPagesToConvert": {
          "type": "integer",
          "description": "Number of pages to convert",
          "format": "int32"
        },
        "ConvertPages": {
          "type": "array",
          "items": {
            "type": "integer",
            "format": "int32"
          },
          "description": "Convert specific pages. The list contains the page indexes of the pages to be converted"
        },
        "WatermarkOptions": {
          "description": "Watermark specific options",
          "$ref": "#/definitions/WatermarkOptionsDto"
        },
        "ShowGridLines": {
          "type": "boolean",
          "description": "Show grid lines when converting Excel files"
        },
        "ShowHiddenSheets": {
          "type": "boolean",
          "description": "Show hidden sheets when converting Excel files"
        },
        "HideWordTrackedChanges": {
          "type": "boolean",
          "description": "Hide markup and track changes for Word documents"
        },
        "CadOptions": {
          "description": "CAD specific convert options",
          "$ref": "#/definitions/CadOptionsDto"
        },
        "HidePdfAnnotations": {
          "type": "boolean",
          "description": "Hide annotations in Pdf documents"
        },
        "HideComments": {
          "type": "boolean",
          "description": "Hide comments during conversion if the source format support comments"
        }
      },
      "description": "Base options class"
    },
    "WatermarkOptionsDto": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "Width",
        "Height",
        "Top",
        "Left",
        "RotationAngle",
        "Transparency"
      ],
      "properties": {
        "Text": {
          "type": "string",
          "description": "Watermark text"
        },
        "Font": {
          "type": "string",
          "description": "Watermark font name"
        },
        "Color": {
          "type": "string",
          "description": "Watermark font color"
        },
        "Width": {
          "type": "integer",
          "description": "Watermark width",
          "format": "int32"
        },
        "Height": {
          "type": "integer",
          "description": "Watermark height",
          "format": "int32"
        },
        "Top": {
          "type": "integer",
          "description": "Watermark top position",
          "format": "int32"
        },
        "Left": {
          "type": "integer",
          "description": "Watermark left position",
          "format": "int32"
        },
        "RotationAngle": {
          "type": "integer",
          "description": "Watermark rotation angle",
          "format": "int32"
        },
        "Transparency": {
          "type": "number",
          "description": "Watermark transparency. Value between 0 and 1. Value 0 is fully visible, value 1 is invisible.",
          "format": "double"
        }
      },
      "description": "Watermark options"
    },
    "CadOptionsDto": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "Width",
        "Height"
      ],
      "properties": {
        "Width": {
          "type": "integer",
          "description": "Set desired page width for converting CAD document",
          "format": "int32"
        },
        "Height": {
          "type": "integer",
          "description": "Set desired page height for converting CAD document",
          "format": "int32"
        },
        "LayoutNames": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "Render specific CAD layouts"
        }
      },
      "description": "Cad Options"
    },
    "BaseConversionRequest": {
      "type": "object",
      "discriminator": "Type",
      "additionalProperties": false,
      "properties": {
        "SourceFile": {
          "description": "The source name",
          "$ref": "#/definitions/ConversionFileInfo"
        }
      },
      "description": "BaseConversionRequest"
    },
    "ConversionFileInfo": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "folder": {
          "type": "string",
          "description": "The folder"
        },
        "name": {
          "type": "string",
          "description": "The name"
        },
        "password": {
          "type": "string",
          "description": "The password"
        }
      },
      "description": "ConversionFileInfo dto"
    },
    "Link": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "href": {
          "type": "string",
          "description": "The \"href\" attribute contains the link's IRI. atom:link elements MUST\nhave an href attribute, whose value MUST be a IRI reference"
        },
        "rel": {
          "type": "string",
          "description": "atom:link elements MAY have a \"rel\" attribute that indicates the link\nrelation type.  If the \"rel\" attribute is not present, the link\nelement MUST be interpreted as if the link relation type is \"alternate\"."
        },
        "type": {
          "type": "string",
          "description": "On the link element, the \"type\" attribute's value is an advisory\nmedia type: it is a hint about the type of the representation that is\nexpected to be returned when the value of the href attribute is\ndereferenced.  Note that the type attribute does not override the\nactual media type returned with the representation."
        },
        "title": {
          "type": "string",
          "description": "The \"title\" attribute conveys human-readable information about the\nlink.  The content of the \"title\" attribute is Language-Sensitive."
        }
      },
      "description": "Provides information for the object link.\nThis is supposed to be an atom:link, therefore it should have all attributes specified here http://tools.ietf.org/html/rfc4287#section-4.2.7"
    },
    "PossibleConversionResponse": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "SourceFileType": {
          "type": "string",
          "description": "Source FileType"
        },
        "PossibleConversions": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "Array with possible conversions"
        }
      },
      "description": "Possible conversions response class"
    },
    "QuickConversionSettings": {
      "type": "null",
      "additionalProperties": false,
      "allOf": [
        {
          "$ref": "#/definitions/BaseConversionRequest"
        },
        {
          "type": "object",
          "properties": {
            "Format": {
              "type": "string",
              "description": "required target format"
            }
          }
        }
      ],
      "description": "QuickConversionSettings"
    },
    "HtmlConversionSettings": {
      "type": "null",
      "additionalProperties": false,
      "allOf": [
        {
          "$ref": "#/definitions/BaseConversionRequest"
        },
        {
          "type": "object",
          "properties": {
            "Options": {
              "description": "The settings",
              "$ref": "#/definitions/HtmlSaveOptionsDto"
            }
          }
        }
      ],
      "description": "HtmlConversionSettings"
    },
    "HtmlSaveOptionsDto": {
      "type": "null",
      "additionalProperties": false,
      "required": [
        "UsePdf",
        "FixedLayout",
        "Zoom"
      ],
      "allOf": [
        {
          "$ref": "#/definitions/SaveOptionsBaseDto"
        },
        {
          "type": "object",
          "properties": {
            "UsePdf": {
              "type": "boolean",
              "description": "If true, the input firstly is converted to PDF and after that to desired format"
            },
            "FixedLayout": {
              "type": "boolean",
              "description": "If true fixed layout will be used e.g. absolutely positioned html elements\nDefault:  true"
            },
